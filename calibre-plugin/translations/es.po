msgid "Basic"
msgstr "Básicas"

#: config.py:254
msgid "Calibre Cover"
msgstr "Portada de calibre"

#: config.py:262
msgid "Standard Columns"
msgstr "Columnas estándar"

#: config.py:265
msgid "Custom Columns"
msgstr "Columnas personalizadas"

#: config.py:268
msgid "Email Settings"
msgstr "Configuración de correo electrónico"

#: config.py:271
