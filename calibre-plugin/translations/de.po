msgstr "Liste der unterstützten Seiten"

#: config.py:196
msgid "FAQs"
msgstr "FAQs"

#: config.py:211
msgid "Basic"
msgstr "Basis"

#: config.py:222
msgid "Calibre Cover"
msgstr "Calibre Cover"

#: config.py:230
msgid "Standard Columns"
msgstr "Standard-Spalten"

#: config.py:233
msgid "Custom Columns"
msgstr "Benutzerdefinierte Spalten"

