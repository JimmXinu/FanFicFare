#: config.py:194
msgid "List of Supported Sites"
msgstr "Lijst van ondersteunde sites"

#: config.py:196
msgid "FAQs"
msgstr "VGV's"

#: config.py:211
msgid "Basic"
msgstr "Basis"

#: config.py:222
msgid "Calibre Cover"
msgstr "Calibre-omslag"

#: config.py:230
msgid "Standard Columns"
msgstr "Standaardkolommen"

#: config.py:233
msgid "Custom Columns"
msgstr "Eigen kolommen"

#: config.py:236
msgid "Email Settings"
msgstr "E-mailinstellingen"

